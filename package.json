{
  "name": "tumulus",
  "version": "0.1.8",
  "private": false,
  "author": "superrache",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "express": "node server/server.dev.js",
    "start": "NODE_ENV=production node server/server.prod.js",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "dependencies": {
    "@turf/along": "^6.5.0",
    "@turf/length": "^6.5.0",
    "@turf/point-on-feature": "^6.5.0",
    "core-js": "^3.6.5",
    "express": "^4.17.3",
    "husky": "^7.0.4",
    "maplibre-gl": "^1.15.2",
    "osm-auth": "^1.1.1",
    "osm-request": "^1.2.10",
    "osmtogeojson": "^3.0.0-beta.4",
    "request": "^2.88.2",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "git-conventional-commits": "^1.2.0",
    "husky": "^7.0.4",
    "standard-version": "^9.3.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": "14.x",
    "npm": "8.1.x"
  },
  "homepage": "https://github.com/superrache/tumulus",
  "bugs": {
    "url": "https://github.com/superrache/tumulus/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "vue",
    "vue.js",
    "geojson",
    "postgresql",
    "map",
    "maplibre",
    "maplibre-gl-js",
    "osm",
    "openstreetmap"
  ],
  "license": "MIT"
}
